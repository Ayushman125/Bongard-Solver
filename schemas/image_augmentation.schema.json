{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Image Augmentation Batch",
  "type": "object",
  "properties": {
    "images": {
      "type": "array",
      "items": {
        "type": "array",
        "items": { "type": "number" }
      }
    },
    "type": {
      "type": "string",
      "enum": ["geometric", "both", "augmented", "original"]
    }
  },
  "required": ["images", "type"]
}
